{
  "name": "@rainbow-me/kit-modal",
  "version": "0.0.23",
  "description": "Universal Ethereum wallet modal library for React",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    "./dist/index.css": "./dist/index.css",
    "./index.css": "./index.css",
    "./package.json": "./package.json",
    "./": {
      "module": "./dist/vanilla-extract-sprinkles.esm.js",
      "default": "./dist/vanilla-extract-sprinkles.cjs.js"
    },
    "./createRuntimeSprinkles": {
      "module": "./createRuntimeSprinkles/dist/vanilla-extract-sprinkles-createRuntimeSprinkles.esm.js",
      "default": "./createRuntimeSprinkles/dist/vanilla-extract-sprinkles-createRuntimeSprinkles.cjs.js"
    },
    "./createUtils": {
      "module": "./createUtils/dist/vanilla-extract-sprinkles-createUtils.esm.js",
      "default": "./createUtils/dist/vanilla-extract-sprinkles-createUtils.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.4"
  },
  "scripts": {
    "build": "node build.js",
    "prepare": "pnpm build",
    "prebuild": "pnpm typegen",
    "typegen": "tsc --emitDeclarationOnly || true"
  },
  "keywords": [
    "ens",
    "ethereum",
    "react",
    "react-hook",
    "hooks",
    "blockchain"
  ],
  "author": "v1rtl",
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@rainbow-me/kit-utils": "workspace:*",
    "@vanilla-extract/css": "^1.6.4",
    "@vanilla-extract/dynamic": "^2.0.0",
    "@web3-react/abstract-connector": "^6.0.7",
    "clsx": "^1.1.1"
  },
  "optionalDependencies": {
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "@web3-react/walletlink-connector": "^6.2.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbow-me/rainbowkit.git",
    "directory": "packages/modal"
  }
}
